
[   {
    "M000": {
        "metadata": {
            "created": "9-4-2025",
            "author": "Trenton Wells",
            "organization": "NREL",
            "NREL_contact": "trenton.wells@nrel.gov",
            "personal_contact": "trentonwells73@gmail.com"
        },
        "terms": {
            "sg": "shape factor (Gaussian)",
            "sl": "shape factor (Lorentzian)",
            "time": "hours in exposure conditions",
            "A": "area under the peak",
            "X": "placeholder degradation condition for future users",
            "A_X": "area under the peak in condition X, index should match time index (so A_X[0] is area at time[0] for condition X)",
            "A3, A4, A5, 0.5_UV_suns, 1_UV_suns, 2_UV_suns, 5_UV_suns, ARC, OPN, KKCE": "degradation conditions",
            "time_condition_X": "placeholder for future condition X that needs its own time array",
            "f": {
                "definition": "fractional Gauss character of the compound peak shape",
                "formula": "f = Ag / At = Ag / (Ag + Al)",
                "component parts": {
                    "Ag": "area under Gaussian appx of peak",
                    "Al": "area under Lorentzian appx of peak",
                    "At": "sum of Ag and Al"
                }
            }
        }
    },
